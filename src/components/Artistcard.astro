---
import "../styles/general.css";
const {data} = Astro.props;
---
<section>
        <img class="cirkel" src={data.img} alt="{alt}">
        <h3>{data.name}</h3>
</section>

<style>
        .cirkel {
        aspect-ratio: 1/1;
        object-fit: cover;
    }

    img {
        width: 100%;
        height: auto;
        box-shadow: 0px 0px 8px 8px #ff4d08;
        border-radius: 50%;
    }

    a {
    text-decoration: none;
    }
</style>